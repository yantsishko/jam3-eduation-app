{
  "name": "BeelineTVE",
  "version": "2.0.3",
  "description": "",
  "repository": {
    "type": "git",
    "url": "https://git.oxagile.com/beelinetve/beelinetve-web.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Yan Tsishko",
  "scripts": {
    "postinstall": "npm run build:dll",
    "prebuild": "npm run build:clean",
    "build": "cross-env NODE_ENV=development webpack --config internals/webpack/webpack.dev.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:prod": "npm run generate:sitemap && cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development && node server",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production && node server",
    "generate:sitemap": "node ./internals/scripts/sitemap.js",
    "presetup": "npm i chalk shelljs",
    "postsetup": "npm run build:dll",
    "clean:all": "npm run analyze:clean && npm run build:clean",
    "lint": "npm run lint:js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern service/ --ignore-pattern app/helpers/",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "babel": {
    "plugins": [
      "styled-components"
    ],
    "presets": [
      [
        "env",
        {
          "modules": false
        }
      ],
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "only": [
          "app"
        ],
        "plugins": [
          "transform-react-remove-prop-types",
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true,
      "jest": true,
      "es6": true
    },
    "plugins": [
      "react",
      "jsx-a11y"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "arrow-parens": [
        "error",
        "always"
      ],
      "import/no-dynamic-require": 0,
      "import/no-webpack-loader-syntax": 0,
      "jsx-a11y/media-has-caption": 0,
      "no-loop-func": 0,
      "no-bitwise": 0,
      "react/forbid-prop-types": 0,
      "react/jsx-filename-extension": 0,
      "react/require-default-props": 0,
      "no-script-url": 0
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./internals/webpack/webpack.prod.babel.js"
        }
      }
    }
  },
  "dllPlugin (disabled)": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "add-asset-html-webpack-plugin": "2.0.1",
    "animate.css": "^3.6.1",
    "babel-polyfill": "6.23.0",
    "chalk": "1.1.3",
    "child_process": "^1.0.2",
    "circular-dependency-plugin": "3.0.0",
    "classnames": "^2.2.5",
    "compression": "1.6.2",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "5.0.0",
    "express": "4.15.3",
    "fontfaceobserver": "2.0.9",
    "git-revision-webpack-plugin": "^2.5.1",
    "glob": "^7.1.2",
    "history": "4.6.3",
    "hoist-non-react-statics": "2.1.1",
    "html-webpack-plugin": "2.29.0",
    "immutable": "3.8.1",
    "invariant": "2.2.2",
    "ip": "1.1.5",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lodash": "4.17.4",
    "md5": "^2.2.1",
    "minimist": "1.2.0",
    "moment": "^2.20.1",
    "ngrok": "2.2.9",
    "offline-plugin": "4.8.1",
    "progress-bar-webpack-plugin": "^1.11.0",
    "prop-types": "15.5.10",
    "qs": "^6.5.1",
    "quill": "^1.3.6",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-quill": "^1.3.3",
    "react-rangeslider": "^2.2.0",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-scroll-parallax": "^1.3.2",
    "react-trigger-change": "^1.0.2",
    "react-visibility-sensor": "^3.11.0",
    "react-ym": "^0.0.7",
    "redux": "3.6.0",
    "redux-immutable": "4.0.0",
    "redux-orm": "^0.9.4",
    "redux-orm-proptypes": "^0.1.0",
    "redux-thunk": "^2.2.0",
    "reselect": "3.0.1",
    "sanitize.css": "4.1.0",
    "sitemap": "^1.13.0",
    "styled-components": "2.0.0",
    "sugarss": "^1.0.1",
    "superagent": "^3.8.1",
    "uid": "^0.0.2",
    "warning": "3.0.0",
    "webpack": "3.5.5",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.0",
    "whatwg-fetch": "2.0.3",
    "xml-loader": "^1.2.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.0",
    "babel-plugin-dynamic-import-node": "1.0.2",
    "babel-plugin-react-intl": "2.3.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-styled-components": "1.1.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-inline-elements": "6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.5",
    "babel-preset-env": "1.5.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "coveralls": "2.13.1",
    "css-loader": "0.28.4",
    "enzyme": "2.8.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-import-resolver-webpack": "0.8.3",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.4",
    "file-loader": "0.11.1",
    "grunt": "^1.0.2",
    "grunt-webfont": "^1.6.0",
    "html-loader": "0.4.5",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.7.1",
    "lint-staged": "3.5.1",
    "node-plop": "0.7.0",
    "null-loader": "0.1.1",
    "plop": "1.8.0",
    "pre-commit": "1.2.2",
    "react-test-renderer": "15.6.1",
    "rimraf": "2.6.1",
    "shelljs": "0.7.7",
    "style-loader": "0.18.1",
    "url-loader": "0.5.8"
  }
}
